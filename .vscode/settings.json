{
  "eslint.options": {
    "configFile": ".eslintrc"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[python]": {
    "editor.guides.indentation": false,
    "editor.tabSize": 2
  }
}
